#!/bin/bash

curl -s -m 10 -Z -x 127.0.0.1:8000 --parallel-immediate --remote-name-all "http://netsys.cs.colorado.edu/graphics/arrowdown.gif" "http://netsys.cs.colorado.edu/graphics/arrowright.gif" "http://netsys.cs.colorado.edu/graphics/arrowup.gif" "http://netsys.cs.colorado.edu/graphics/changes.gif" "http://netsys.cs.colorado.edu/graphics/doc.gif" "http://netsys.cs.colorado.edu/graphics/edittopic.gif" "http://netsys.cs.colorado.edu/graphics/else.gif" "http://netsys.cs.colorado.edu/graphics/filter.gif" "http://netsys.cs.colorado.edu/graphics/gif.gif" "http://netsys.cs.colorado.edu/graphics/group.gif" "http://netsys.cs.colorado.edu/graphics/help.gif" "http://netsys.cs.colorado.edu/graphics/home.gif" "http://netsys.cs.colorado.edu/graphics/html.gif" "http://netsys.cs.colorado.edu/graphics/index.gif" "http://netsys.cs.colorado.edu/graphics/indexlist.gif" "http://netsys.cs.colorado.edu/graphics/info.gif" "http://netsys.cs.colorado.edu/graphics/jpg.gif" "http://netsys.cs.colorado.edu/graphics/more-small.gif" "http://netsys.cs.colorado.edu/graphics/move.gif" "http://netsys.cs.colorado.edu/graphics/mov.gif" "http://netsys.cs.colorado.edu/graphics/newtopic.gif" "http://netsys.cs.colorado.edu/graphics/notify.gif" "http://netsys.cs.colorado.edu/graphics/pdf.gif" "http://netsys.cs.colorado.edu/graphics/person.gif" "http://netsys.cs.colorado.edu/graphics/png.gif" "http://netsys.cs.colorado.edu/graphics/ppt.gif" "http://netsys.cs.colorado.edu/graphics/recentchanges.gif" "http://netsys.cs.colorado.edu/graphics/rss-small.gif" "http://netsys.cs.colorado.edu/graphics/search-small.gif" "http://netsys.cs.colorado.edu/graphics/searchtopic.gif" "http://netsys.cs.colorado.edu/graphics/statistics.gif" "http://netsys.cs.colorado.edu/graphics/tablesortdiamond.gif" "http://netsys.cs.colorado.edu/graphics/tablesortdown.gif" "http://netsys.cs.colorado.edu/graphics/tip.gif" "http://netsys.cs.colorado.edu/graphics/topicbacklinks.gif" "http://netsys.cs.colorado.edu/graphics/topicdiffs.gif" "http://netsys.cs.colorado.edu/graphics/trash.gif" "http://netsys.cs.colorado.edu/graphics/txt.gif" "http://netsys.cs.colorado.edu/graphics/viewtopic.gif" "http://netsys.cs.colorado.edu/graphics/warning.gif" "http://netsys.cs.colorado.edu/graphics/web-bg.gif" "http://netsys.cs.colorado.edu/graphics/web-bg-small.gif" "http://netsys.cs.colorado.edu/graphics/wrench.gif" "http://netsys.cs.colorado.edu/graphics/zip.gif"

for file in $(find . -name "*.gif" -type f); do
    diff $file ../PA2/www/graphics/$file
    if [ $? -eq 0 ]; then
        rm $file
    fi
done