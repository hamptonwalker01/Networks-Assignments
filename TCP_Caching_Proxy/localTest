#!/bin/bash
curl -s -m 10 -Z -x 127.0.0.1:8000 --parallel-immediate --remote-name-all "http://localhost:8888/graphics/arrowdown.gif" "http://localhost:8888/graphics/arrowright.gif" "http://localhost:8888/graphics/arrowup.gif" "http://localhost:8888/graphics/changes.gif" "http://localhost:8888/graphics/doc.gif" "http://localhost:8888/graphics/edittopic.gif" "http://localhost:8888/graphics/else.gif" "http://localhost:8888/graphics/filter.gif" "http://localhost:8888/graphics/gif.gif" "http://localhost:8888/graphics/group.gif" "http://localhost:8888/graphics/help.gif" "http://localhost:8888/graphics/home.gif" "http://localhost:8888/graphics/html.gif" "http://localhost:8888/graphics/index.gif" "http://localhost:8888/graphics/indexlist.gif" "http://localhost:8888/graphics/info.gif" "http://localhost:8888/graphics/jpg.gif" "http://localhost:8888/graphics/more-small.gif" "http://localhost:8888/graphics/move.gif" "http://localhost:8888/graphics/mov.gif" "http://localhost:8888/graphics/newtopic.gif" "http://localhost:8888/graphics/notify.gif" "http://localhost:8888/graphics/pdf.gif" "http://localhost:8888/graphics/person.gif" "http://localhost:8888/graphics/png.gif" "http://localhost:8888/graphics/ppt.gif" "http://localhost:8888/graphics/recentchanges.gif" "http://localhost:8888/graphics/rss-small.gif" "http://localhost:8888/graphics/search-small.gif" "http://localhost:8888/graphics/searchtopic.gif" "http://localhost:8888/graphics/statistics.gif" "http://localhost:8888/graphics/tablesortdiamond.gif" "http://localhost:8888/graphics/tablesortdown.gif" "http://localhost:8888/graphics/tip.gif" "http://localhost:8888/graphics/topicbacklinks.gif" "http://localhost:8888/graphics/topicdiffs.gif" "http://localhost:8888/graphics/trash.gif" "http://localhost:8888/graphics/txt.gif" "http://localhost:8888/graphics/viewtopic.gif" "http://localhost:8888/graphics/warning.gif" "http://localhost:8888/graphics/web-bg.gif" "http://localhost:8888/graphics/web-bg-small.gif" "http://localhost:8888/graphics/wrench.gif" "http://localhost:8888/graphics/zip.gif"

for file in $(find . -name "*.gif" -type f); do
    diff $file ../PA2/www/graphics/$file
    if [ $? -eq 0 ]; then
        rm $file
    fi
done

